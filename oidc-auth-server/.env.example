# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here  
GOOGLE_REDIRECT_URI=https://oidc.drjlabs.com/auth/callback

# OIDC Server Configuration
OIDC_BASE_URL=https://oidc.drjlabs.com

# RSA Key Configuration (Production)
# Use either RSA_PRIVATE_KEY_PEM (environment variable) or RSA_PRIVATE_KEY_PATH (file mount)
# RSA_PRIVATE_KEY_PEM=-----BEGIN PRIVATE KEY-----...
# RSA_PRIVATE_KEY_PATH=/run/secrets/rsa_private_key
JWT_KEY_ID=key1

# Environment
ENVIRONMENT=production 